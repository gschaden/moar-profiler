{
  "name": "moar-profiler",
  "version": "1.0.0",
  "description": "v8 cpu profiler with moar",
  "license": "MIT",
  "author": "Patrick Mueller <pmuellr@apache.org> (https://github.com/pmuellr)",
  "homepage": "https://github.com/pmuellr/moar-profiler",
  "main": "moar-profiler.js",
  "bin": {
    "moar-profiler": "./moar-profiler.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pmuellr/moar-profiler.git"
  },
  "bugs": {
    "url": "https://github.com/pmuellr/moar-profiler/issues"
  },
  "scripts": {
    "standard": "echo 'running standard' && standard -v",
    "test": "npm -s run standard && npm -s run utest",
    "utest": "INSPECTOR_PORT=9228 node --inspect=9228 test/index.js | FORCE_COLOR=1 tap-spec",
    "watch": "nodemon --exec 'npm -s test'"
  },
  "standard": {
    "ignore": [
      "/tmp/",
      "/node_modules/"
    ]
  },
  "dependencies": {
    "async": "~2.5.0",
    "inspector-remote": "github:pmuellr/inspector-remote",
    "minimist": "~1.2.0"
  },
  "devDependencies": {
    "nodemon": "~1.11.0",
    "standard": "~10.0.0",
    "tap-spec": "~4.1.1",
    "tape": "~4.7.0"
  }
}
